{
  "manifest_version": 3,
  "name": "Productivity extension",
  "version": "1.0",
  "description": "Blocker for distractive websites",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "webNavigation", "storage"]
}
